
<div class="row">
  <div class="col s12 m9 l9">
    <div class="row">
      <% @articles.each do |article| %>
        <div class="col s12 m12 l6">
          <%= render partial: "articles/card", object: article, as: 'article' %>
        </div>
      <% end %>
    </div>
  </div>
  <div class="col s12 m3 l3">
    <%= render "layouts/aside" %>
  </div>
</div>
